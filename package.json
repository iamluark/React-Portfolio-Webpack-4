{
  "name": "iamluark-cv-react",
  "version": "1.0.0",
  "description": "react-cv",
  "main": "index.js",
  "homepage": "https://iamluark.github.io",
  "scripts": {
    "build-css": "node-sass-chokidar ./src/assets/scss -o ./src/assets/scss",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/assets/scss -o ./src/assets/scss --watch",
    "start-js": "webpack-dev-server --open --hot --mode development",
    "start": "npm-run-all -p watch-css start-js",
    "build": "webpack --mode production && npm run build-css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build"
  },
  "author": "CLark Dwain Luna",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^1.2.2",
    "@material-ui/icons": "^1.1.0",
    "ajv": "^6.5.1",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.0",
    "npm": "^6.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
